{
    "workbench.startupEditor": "newUntitledFile",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 1,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.autoClosingOvertype": "always",
    "eslint.packageManager": "pnpm",
    "eslint.runtime": "/usr/bin/node",
    "editor.fontFamily": "'JetBrains Mono', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.insertSpaces": false,
    "editor.smoothScrolling": true,
    "editor.fontLigatures": true,
    "telemetry.enableCrashReporter": false,
    "eslint.alwaysShowStatus": true,
    "eslint.format.enable": true,
    "materialTheme.accent": "Teal",
    "workbench.iconTheme": "eq-material-theme-icons-light",
    "workbench.colorTheme": "Atom One Light",
    "workbench.tree.indent": 20,
    "python.languageServer": "Microsoft",
    "python.pythonPath": "/usr/bin/python3",
    "workbench.colorCustomizations": {
        "tab.inactiveBackground": "#f2f3f5",
        "tab.border": "#cfd0d1",
        "activityBar.background": "#23272A",
        "activityBar.foreground": "#7289DA",
        "activityBar.inactiveForeground": "#4E5D94",
        "sideBar.background": "#f2f3f5",
        "sideBar.foreground": "#060c3c",
        "sideBar.border": "#cfd0d1",
        "sideBarSectionHeader.background": "#ECECEC",
        "statusBar.background": "#23272A",
        "statusBar.foreground": "#7289DA",
    },
    "prettier.packageManager": "pnpm",
    "prettier.tabWidth": 4,
    "prettier.useTabs": true
}
